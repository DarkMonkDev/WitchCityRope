# Configuration for @witchcityrope/shared-types package
# Copy to .env and customize as needed

# API Base URL for type generation (automatically detects running API)
VITE_API_BASE_URL=http://localhost:5656

# Alternative API ports to try (comma-separated)
API_DISCOVERY_PORTS=5656,5655,5653

# Generation settings
SKIP_TYPE_VALIDATION=false
ENABLE_DEBUG_LOGGING=false