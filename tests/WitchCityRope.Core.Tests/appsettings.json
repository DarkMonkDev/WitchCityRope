{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Security": {
    "EncryptionKey": "CORE-TESTS-ENCRYPTION-KEY-32CHAR",
    "PasswordMinLength": 8,
    "PasswordRequireDigit": true,
    "PasswordRequireUppercase": true,
    "PasswordRequireNonAlphanumeric": true
  },
  "TestSettings": {
    "UseStubServices": true,
    "EnableDetailedLogging": false
  }
}